MCP_SERVER_URL=http://localhost:8000/mcp
OPENAI_API_KEY=sk-your-openai-api-key
OPENAI_MODEL=gpt-4o-mini

# The name of the header and cookie which contain JWT details
JWT_HEADER_NAME=Authorization
COOKIE_NAME=vacay_auth_token

# OAuth Provider Endpoints (Keycloak)
AUTH_ENDPOINT=http://your-idp:8080/realms/yourrealm/protocol/openid-connect/auth
TOKEN_ENDPOINT=http://your-idp:8080/realms/yourrealm/protocol/openid-connect/token
JWKS_URL=http://your-idp:8080/realms/yourrealm/protocol/openid-connect/certs

# Application Configuration
CLIENT_ID=your-client-id
CLIENT_SECRET=your-client-secret
OAUTH_REDIR_URI=/auth/callback

# OAuth Parameters
OAUTH_STATE=change-me
OAUTH_SCOPE=openid

# Frontend redirect after login
CUR_HOSTNAME=http://localhost:3000
FRONTEND_URL=http://localhost:3000
PORT=3000
